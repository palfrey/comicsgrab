strip ctrlaltdel
	desc Ctrl+Alt+Del
	type search
	homepage http://www.cad-comic.com/cad/
	days Mon Wed Fri Sat
	subbeg
		searchpattern <img.+?src="(http://img.ctrlaltdel-online.com/comics/[^\"]+)"
	subend
	subbeg
		searchpattern src="(/comics/[^\"]+)"
	subend
	subbeg
		searchpattern (http://.*?.cad-comic.com/comics/[^\"]+)
	subend
end
