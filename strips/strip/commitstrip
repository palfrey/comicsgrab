strip commitstrip
	desc CommitStrip
	type search
	searchpage $infoval
	searchpattern (/wp-content/uploads/\d{4}/\d{2}/Strip[^\"]+)
	infopage http://www.commitstrip.com/en/
	homepage http://www.commitstrip.com/en/
	baseurl http://www.commitstrip.com/en/
	infoval <meta property=\"og:url\" content=\"(http://www.commitstrip.com/en/\d{4}[^\"]+)"/>
end
