class btphp
	type search
	baseurl $homepage
	subbeg
		searchpattern (comic.php\?id=\d+)&amp;save-as=\d+.(?:jpg|png|gif)
	subend
	subbeg
		searchpattern <img src="(comic.php\?id=.*?\.(?:jpg|png|gif))
	subend
end

class comics-sherpa-ucomics
	type search
	searchpattern <img.+?src="(http://images.ucomics.com/comics/$1/\d{4}/$1\d{6}.gif)
	homepage http://www.comicssherpa.com/site/feature?uc_comic=$1
end

class comicspage-editorial-gen
	type generate
	homepage http://www.comicspage.com/$1/index.html
	imageurl http://www.tmsfeatures.com/tmsfeatures/servlet/com.featureserv.util.Download?file=%Y%m%ded$2-a-p.jpg&code=ed$2
end

class comicspage-panels-gen
	type generate
	homepage http://www.comicspage.com/$1/index.html
	imageurl http://www.tmsfeatures.com/tmsfeatures/servlet/com.featureserv.util.Download?file=%Y%m%dcp$2-<code:"s" if %w==0><code:"a" if %w!=0>-p.jpg&code=cp$2
end

class comicspage-strips-gen
	type generate
	homepage http://www.comicspage.com/$1/index.html
	imageurl http://www.tmsfeatures.com/tmsfeatures/servlet/com.featureserv.util.Download?file=%Y%m%dcs$2-<code:"s" if %w==0><code:"a" if %w!=0>-p.jpg&code=cs$2
	referer http://www.comicspage.com/$1/index.html
end

class ioncomix
	type search
	searchpattern <img.+?src='(/index.php\?do_command=show_strip\&strip_id=\d+\&auth=[^']+)'
	baseurl $homepage
end

class keenspace
	type search
	subbeg
		baseurl $homepage
		searchpattern document.write\('<(?:IMG|img)[^S]*SRC="(/comics/$1[^\"]*)"
	subend
	subbeg
		searchpattern <(?:IMG|img).*?SRC="($homepage/comics/$1[^\"]*)"
	subend
	subbeg
		baseurl $homepage
		searchpattern <(?:IMG|img).*?SRC="(/?comics/$1[^\"]*)"
	subend
end

class mercurycenter
