# Class definitions

class ioncomix
	baseurl $homepage
	type search
	searchpattern <img.+?src='(/index.php\?do_command=show_strip\&strip_id=\d+\&auth=[^']+)'
end

# Works for all keenspace comics that have the /comics/name pattern - homepages vary tho.
# $1 = name bit
class keenspace
	type search
	subbeg
		baseurl $homepage
		searchpattern document.write\('<(?:IMG|img)[^S]*SRC="(/comics/$1[^\"]*)"
	subend
	subbeg
		searchpattern <(?:IMG|img).*?SRC="($homepage/comics/$1[^\"]*)"
	subend
	subbeg
		baseurl $homepage
		searchpattern <(?:IMG|img).*?SRC="(/?comics/$1[^\"]*)"
	subend
end

class ucomics-srch
	homepage http://www.gocomics.com/$strip/
	type search
	#searchpattern <img.+?src=".+?/($1/(?:\d+)/$1(?:\d+).+?)"
	#baseurl http://images.ucomics.com/comics/

	# Magic sequence is from http://poncho.ucomics.com/xml/xmldrm/
	infopage http://poncho.ucomics.com/xml/xmldrm/
	infoval <key>([\d+a-z]*)</key>
	searchpattern http://images.gocomics.com/images/drm/comicViewerUc\?w=\d+&h=\d+&k=([\da-f]+)
	baseurl http://poncho.ucomics.com/view/$infoval/
	index 1
end

